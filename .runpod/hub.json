{
  "title": "MiniGPT4 RunPod",
  "description": "MiniGPT-4 Serverless Worker",
  "type": "serverless",
  "category": "image",
  "iconUrl": "https://github.com/Vision-CAIR/MiniGPT-4/raw/main/minigpt4_logo.png",
  "config": {
    "runsOn": "GPU",
    "containerDiskInGb": 100,
    "presets": [
      {
        "name": "Default",
        "defaults": {
          "MODEL_PATH": "checkpoints/mini-gpt4-7b/model.pth",
          "CFG_PATH": "eval_configs/minigpt4_eval.yaml"
        }
      }
    ],
    "env": [
      {
        "key": "MODEL_PATH",
        "value": "/runpod-volume/checkpoints/mini-gpt4-7b/model.pth"
      },
      {
        "key": "MODEL_URL",
        "value": "https://huggingface.co/Vision-CAIR/MiniGPT-4/resolve/main/model.pth"
      },
      {
        "key": "LLAMA_MODEL",
        "value": "lmsys/vicuna-7b-v1.3"
      },
      {
        "key": "CFG_PATH",
        "value": "eval_configs/minigpt4_eval.yaml"
      },
      {
        "key": "HF_HOME",
        "value": "/runpod-volume/cache"
      },
      {
        "key": "HF_HUB_CACHE",
        "value": "/runpod-volume/cache/hub"
      },
      {
        "key": "TRANSFORMERS_CACHE",
        "value": "/runpod-volume/cache/transformers"
      }
    ]
  }
}
